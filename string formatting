def print_formatted(number):
    width = len(bin(number)[2: ])
    for i in range(1,number + 1):
        b =bin(i)[2:]
        d =str(i)
        o=oct(i)[2:]
        h=hex(i)[2:].upper()
        print(d.rjust(width),o.rjust(width),h.rjust(width),b.rjust(width))# your code goes here

if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
